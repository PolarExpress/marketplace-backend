{
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coverageReporters": [
    "lcov"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": 80
    }
  },
  "clearMocks": true,
  "preset": "ts-jest",
  "testEnvironment": "node",
  "testPathIgnorePatterns": [
    "node_modules",
    "build",
    "data"
  ]
}