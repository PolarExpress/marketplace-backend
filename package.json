{
  "name": "polarexpress-backend",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "dev": "node -r ts-node/register -r dotenv/config src/index.ts",
    "start": "node build/index.js",
    "seed": "node -r ts-node/register -r dotenv/config seed.ts",
    "test": "jest --passWithNoTests",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "prepare": "husky || echo \"Skipping husky in production\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "4.19.2",
    "minio": "^7.1.3",
    "mongodb": "^6.5.0",
    "ts-amqp-socket": "github:PolarExpress/ts-amqp-socket",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ngneat/falso": "^7.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.19",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-perfectionist": "^2.10.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "mongodb-memory-server": "^9.1.8",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "ts-jest": "^29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "typescript-eslint": "^7.1.0"
  }
}
