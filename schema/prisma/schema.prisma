datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
    binaryTargets = ["linux-musl-openssl-3.0.x", "debian-openssl-3.0.x"]
}

enum AddonCategory {
    VISUALISATION
    MACHINE_LEARNING
    DATA_SOURCE
}

model User {
    id String @id() @default(cuid())
    /// @email
    email String @unique()
    installs Addon[]
}

model Addon {
    id String @id() @default(cuid())
    name String
    summary String
    icon String
    category AddonCategory
    installedBy User[]
}